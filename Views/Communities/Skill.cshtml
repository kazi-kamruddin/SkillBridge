@{
    ViewBag.Title = ViewBag.SkillName + " Community";
}

<div class="container mt-5">
    <h2 class="match-heading">@ViewBag.SkillName Community</h2>
    <p>Welcome to the @ViewBag.SkillName community!</p>

    <div class="alert alert-info">
        Posts and discussions for <strong>@ViewBag.SkillName</strong> will appear here.
    </div>
</div>
